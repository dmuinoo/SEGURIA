      <bloque id="ciii" tipo="encabezado" titulo="CAPÍTULO III">
        <version id_norma="BOE-A-1995-27964" fecha_publicacion="19951230" fecha_vigencia="19960101">
          <p class="capitulo_num">CAPÍTULO III</p>
          <p class="capitulo_tit">Otros tributos</p>
        </version>
