      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2016-478" fecha_publicacion="20151230" fecha_vigencia="20160101">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Tributos Propios</p>
