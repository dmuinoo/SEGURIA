        <version id_norma="BOE-A-2012-15564" fecha_publicacion="20121226" fecha_vigencia="20130101">
          <p class="anexo_num">ANEXO 1</p>
          <p class="anexo_tit">Límites de las ayudas por traslado de enfermos, salud mental y productos sanitarios para diabéticos</p>
          <table class="tabla" border="0" summary="Tabla de datos">
            <tbody>
              <tr>
                <th>
                  <p class="cabeza_tabla">Prestación</p>
                </th>
                <th>
                  <p class="cabeza_tabla">Límite de la ayuda</p>
