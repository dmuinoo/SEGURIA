      <bloque id="anii" tipo="encabezado" titulo="ANEXO II">
        <version id_norma="BOE-A-2004-3629" fecha_publicacion="20040227" fecha_vigencia="20040228">
          <p class="anexo_num">ANEXO II</p>
          <p class="anexo_tit">Solicitud de acceso a las pruebas de certificación y de expedición de certificados de profesionalidad</p>
          <p class="imagen">
