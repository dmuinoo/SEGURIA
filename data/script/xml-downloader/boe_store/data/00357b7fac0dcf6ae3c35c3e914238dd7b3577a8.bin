      <bloque id="cv-4" tipo="encabezado" titulo="CAPÍTULO VIII">
        <version id_norma="BOE-A-1993-2877" fecha_publicacion="19921202" fecha_vigencia="19930101">
          <p class="capitulo_num">CAPÍTULO VIII</p>
          <p class="capitulo_tit">Gestión del Impuesto</p>
