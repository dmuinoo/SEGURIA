      <bloque id="s5-2" tipo="encabezado" titulo="Sección 5">
        <version id_norma="BOE-A-1995-18266" fecha_publicacion="19950728" fecha_vigencia="19951001">
          <p class="seccion">Sección 5.ª Deber de colaboración</p>
        </version>
