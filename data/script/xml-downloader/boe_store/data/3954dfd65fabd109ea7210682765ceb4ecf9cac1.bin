      <bloque id="s7" tipo="encabezado" titulo="Sección 7">
        <version id_norma="BOE-A-1995-18266" fecha_publicacion="19950728" fecha_vigencia="19951001">
          <p class="seccion">Sección 7.ª Repercusión</p>
