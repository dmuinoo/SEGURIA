      <bloque id="s1-20" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2017-10295" fecha_publicacion="20170719" fecha_vigencia="20170901">
          <p class="seccion">Sección 1.ª Tipos generales de infracciones y sanciones</p>
