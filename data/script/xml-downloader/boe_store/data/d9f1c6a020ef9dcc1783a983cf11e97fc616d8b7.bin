      <bloque id="aniii" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2005-5755" fecha_publicacion="20050409" fecha_vigencia="20050410">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Características de los servicios y tipos de tren</p>
