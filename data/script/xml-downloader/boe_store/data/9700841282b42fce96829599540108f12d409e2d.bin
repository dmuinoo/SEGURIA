      <bloque id="s2-2" tipo="precepto" titulo="SECCIÓN 2">
        <version id_norma="BOE-A-1998-16718" fecha_publicacion="19980714" fecha_vigencia="19981214">
          <p class="seccion">Sección 2.ª Recurso ordinario de apelación</p>
        </version>
