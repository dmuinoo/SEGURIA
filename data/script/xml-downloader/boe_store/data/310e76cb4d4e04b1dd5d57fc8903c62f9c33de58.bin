      <bloque id="cii-2" tipo="encabezado" titulo="CAPÍTULO II">
        <version id_norma="BOE-A-2007-15158" fecha_publicacion="20070720" fecha_vigencia="20080120">
          <p class="capitulo_num">CAPÍTULO II</p>
          <p class="capitulo_tit">Prevención y control ambiental</p>
        </version>
      </bloque>
      <bloque id="s1" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2007-15158" fecha_publicacion="20070720" fecha_vigencia="20080120">
          <p class="seccion">Sección 1.ª Definiciones</p>
        </version>
