      <bloque id="s1-2" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2006-20767" fecha_publicacion="20061109" fecha_vigencia="20070101">
          <p class="seccion">Sección 1 .ª Contenido y principios de ordenación</p>
