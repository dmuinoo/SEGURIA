        <version id_norma="BOE-A-2021-5210" fecha_publicacion="20210226" fecha_vigencia="20210318">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Simplificación de la tramitación administrativa autonómica</p>
        </version>
      </bloque>
      <bloque id="s1" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2021-5210" fecha_publicacion="20210226" fecha_vigencia="20210318">
          <p class="seccion">Sección 1.ª Simplificación de la tramitación de los informes sectoriales necesarios para la implantación de las iniciativas empresariales</p>
        </version>
