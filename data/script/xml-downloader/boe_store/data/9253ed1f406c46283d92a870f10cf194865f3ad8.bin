      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2011-7405" fecha_publicacion="20110408" fecha_vigencia="20110708">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Denominaciones de calidad diferenciada</p>
        </version>
        <version id_norma="BOE-A-2020-90058" fecha_publicacion="20200312" fecha_vigencia="20200313">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Regímenes de calidad diferenciada</p>
          <blockquote>
            <p class="nota_pie">Se modifica el enunciado por el art. 16.3 del Decreto-ley 2/2020, de 9 de marzo. <a class="refPost">Ref. BOJA-b-2020-90058</a></p>
          </blockquote>
