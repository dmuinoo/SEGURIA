        <version id_norma="BOE-A-2005-10622" fecha_publicacion="20050623" fecha_vigencia="20050623">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Nuevas actuaciones de interés general</p>
          <p class="parrafo_2">1. CUENCA HIDROGRÁFICA DEL SUR.</p>
