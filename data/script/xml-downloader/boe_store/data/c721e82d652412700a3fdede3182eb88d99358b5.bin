      <bloque id="s1" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2004-11836" fecha_publicacion="20040625" fecha_vigencia="20040626">
          <p class="seccion">Sección 1.ª Gestión Recaudatoria y Órganos de Recaudación</p>
