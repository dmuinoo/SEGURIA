      <bloque id="si-2" tipo="encabezado" titulo="Sección I">
        <version id_norma="BOE-A-2009-14964" fecha_publicacion="20090909" fecha_vigencia="20090910">
          <p class="seccion">Sección I. Tasa fiscal sobre los juegos de suerte, envite o azar</p>
