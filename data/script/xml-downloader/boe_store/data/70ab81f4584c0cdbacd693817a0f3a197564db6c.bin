            <colgroup>
              <col class="row_column_1"/>
              <col class="row_column_2"/>
              <col class="row_column_2"/>
              <col class="row_column_2"/>
              <col class="row_column_3"/>
            </colgroup>
            <thead>
              <tr class="row_column_4">
                <td>
                  <p class="cabeza_tabla">Grupos de productos</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Junio 2020</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Julio 2020</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Agosto 2020</p>
                </td>
                <td>
