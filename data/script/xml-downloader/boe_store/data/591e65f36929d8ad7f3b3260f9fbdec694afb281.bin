      <bloque id="ss1-3" tipo="encabezado" titulo="Subsección 1">
        <version id_norma="BOE-A-1994-14960" fecha_publicacion="19940629" fecha_vigencia="19940901">
          <p class="subseccion">Subsección 1. Disposición general</p>
        </version>
