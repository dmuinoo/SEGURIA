      <bloque id="cvi-3" tipo="encabezado" titulo="CAPÍTULO VI">
        <version id_norma="BOE-A-1994-14960" fecha_publicacion="19940629" fecha_vigencia="19940901">
          <p class="capitulo_num">CAPÍTULO VI</p>
          <p class="capitulo_tit">Régimen de obligaciones, infracciones y sanciones</p>
