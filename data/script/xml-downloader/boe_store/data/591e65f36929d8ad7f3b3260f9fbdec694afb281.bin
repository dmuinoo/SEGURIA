        <version id_norma="BOE-A-1994-14960" fecha_publicacion="19940629" fecha_vigencia="19940901">
          <p class="seccion">Sección 3. Revalorización e importes máximos y mínimos de pensiones</p>
        </version>
        <version id_norma="BOE-A-2015-11644" fecha_publicacion="20151030" fecha_vigencia="20160101">
