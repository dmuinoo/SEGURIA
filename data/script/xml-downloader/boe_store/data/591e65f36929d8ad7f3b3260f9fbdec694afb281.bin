      <bloque id="s2-13" tipo="precepto" fecha_caducidad="20031211" titulo="SECCIÓN 2">
        <version id_norma="BOE-A-1994-14960" fecha_publicacion="19940629" fecha_vigencia="19940901">
          <p class="seccion">Sección 2. Modalidad no contributiva</p>
        </version>
