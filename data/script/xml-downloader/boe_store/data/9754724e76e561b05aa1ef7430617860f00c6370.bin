      <bloque id="s1-7" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-1998-23233" fecha_publicacion="19980626" fecha_vigencia="19980627">
          <p class="seccion">Sección 1.ª Tasa por prestación de servicios facultativos veterinarios</p>
