      <bloque id="s8" tipo="encabezado" titulo="Sección 8">
        <version id_norma="BOE-A-1998-23233" fecha_publicacion="19980626" fecha_vigencia="19980627">
          <p class="seccion">Sección 8.ª Tasa por permisos de pesca</p>
