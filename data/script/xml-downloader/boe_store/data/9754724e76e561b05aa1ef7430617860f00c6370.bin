      <bloque id="s3" tipo="encabezado" titulo="Sección 3">
        <version id_norma="BOE-A-1998-23233" fecha_publicacion="19980626" fecha_vigencia="19980627">
          <p class="seccion">Sección 3.ª Tasa de minas</p>
