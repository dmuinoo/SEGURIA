        <version id_norma="BOE-A-2010-19154" fecha_publicacion="20101213" fecha_vigencia="20101214">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Situación de incapacidad temporal - Informe médico adicional de ratificación</p>
          <p class="imagen">
            <img class="imagen" src="/datos/imagenes/disp/2010/302/19154_008.png" alt="Imagen: img/disp/2010/302/19154_008.png"/>
