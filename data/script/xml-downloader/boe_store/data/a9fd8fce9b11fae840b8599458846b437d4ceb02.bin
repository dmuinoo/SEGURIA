      <bloque id="s1-3" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2000-1011" fecha_publicacion="19991228" fecha_vigencia="19991229">
          <p class="seccion">Sección 1.ª Composición y funciones</p>
        </version>
