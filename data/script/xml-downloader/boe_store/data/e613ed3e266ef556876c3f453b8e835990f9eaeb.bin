      <bloque id="anx" tipo="encabezado" titulo="ANEXO X">
        <version id_norma="BOE-A-2015-4218" fecha_publicacion="20150418" fecha_vigencia="20150419">
          <p class="anexo_num">ANEXO X</p>
          <p class="anexo_tit">Valores equivalentes de emisión para los métodos de reducción de emisiones</p>
          <table class="tabla" border="0" summary="Tabla de datos">
            <tbody>
              <tr>
                <th>
                  <p class="cabeza_tabla">Contenido de azufre de los combustibles para uso marítimo (% m/m)</p>
                </th>
                <th>
                  <p class="cabeza_tabla">Relación de emisiones de SO<sub>2</sub> (ppm)/CO<sub>2</sub> (% v/v)</p>
                </th>
              </tr>
              <tr>
                <td>
                  <p class="cuerpo_tabla_der">3,50</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_der">151,7</p>
                </td>
              </tr>
              <tr>
