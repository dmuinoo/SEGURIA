            <tr>
              <td>
                <p class="cuerpo_tabla_centro"> </p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro"> </p>
              </td>
              <td>
                <p class="cabeza_tabla">Euros</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">1.</p>
              </td>
              <td>
                <p class="cuerpo_tabla_izq">Habilitación nacional de Profesores Titulares de Escuelas Universitarias</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">24,04</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">2.</p>
              </td>
