        <version id_norma="BOE-A-2007-18770" fecha_publicacion="20071030" fecha_vigencia="20071031">
          <p class="anexo_num">ANEXO II</p>
          <p class="anexo_tit">Materias básicas por rama de conocimiento</p>
          <p class="subseccion">Artes y Humanidades</p>
          <p class="parrafo">Antropología.</p>
          <p class="parrafo">Arte.</p>
