      <bloque id="s4-3" tipo="encabezado" titulo="SECCIÓN 4">
        <version id_norma="BOE-A-1995-7730" fecha_publicacion="19950329" fecha_vigencia="19950501">
          <p class="seccion">Sección 4.ª Extinción del contrato</p>
