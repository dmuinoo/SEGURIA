      <bloque id="ci-8" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2024-26915" fecha_publicacion="20241224" fecha_vigencia="20241225">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Medidas sobre pensiones y otras prestaciones públicas</p>
