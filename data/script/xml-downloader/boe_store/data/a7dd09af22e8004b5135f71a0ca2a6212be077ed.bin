      <bloque id="s1" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2024-26915" fecha_publicacion="20241224" fecha_vigencia="20241225">
          <p class="seccion">Sección 1.ª Entidades Locales</p>
        </version>
