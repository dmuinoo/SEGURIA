            <tr>
              <th>
                <p class="cabeza_tabla">Importe neto de la cifra de negocios (euros)</p>
              </th>
              <th>
                <p class="cabeza_tabla">Coeficiente</p>
              </th>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">Desde 1.000.000,00 hasta 5.000.000,00</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">1,29</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">Desde 5.000.000,01 hasta 10.000.000,00</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">1,30</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">Desde 10.000.000,01 hasta 50.000.000,00</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">1,32</p>
              </td>
            </tr>
