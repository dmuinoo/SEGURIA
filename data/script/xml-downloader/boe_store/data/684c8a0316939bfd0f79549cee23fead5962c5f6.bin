      <bloque id="s2" tipo="encabezado" titulo="Sección 2">
        <version id_norma="BOE-A-2011-18161" fecha_publicacion="20111020" fecha_vigencia="20111021">
          <p class="seccion">Sección 2.ª Reducciones en la base imponible en adquisiciones lucrativas entre vivos</p>
        </version>
