      <bloque id="s1-14" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2015-11724" fecha_publicacion="20151031" fecha_vigencia="20160102">
          <p class="seccion">Sección 1.ª Prestaciones</p>
        </version>
