            <thead>
              <tr class="row_column_15">
                <td>
                  <p class="cabeza_tabla">Posiciones</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Naturaleza</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Descripción de los campos</p>
