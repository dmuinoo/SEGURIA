        <version id_norma="BOE-A-2011-17974" fecha_publicacion="20111117" fecha_vigencia="20111118">
          <p class="anexo_num">ANEXO I</p>
          <p class="anexo_tit">Reglamentación básica de «Juegos Complementarios»</p>
        </version>
      </bloque>
      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2011-17974" fecha_publicacion="20111117" fecha_vigencia="20111118">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Disposiciones generales</p>
        </version>
