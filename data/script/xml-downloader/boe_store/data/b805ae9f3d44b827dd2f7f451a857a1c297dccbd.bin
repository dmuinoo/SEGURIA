        <version id_norma="BOE-A-1995-19108" fecha_publicacion="19950530" fecha_vigencia="19950530">
          <p class="titulo_num">TÍTULO X</p>
          <p class="titulo_tit">De las infracciones y sanciones</p>
        </version>
