      <bloque id="civ-2" tipo="encabezado" titulo="CAPÍTULO IV">
        <version id_norma="BOE-A-2006-20764" fecha_publicacion="20061129" fecha_vigencia="20070101">
          <p class="capitulo_num">CAPÍTULO IV</p>
          <p class="capitulo_tit">Obligaciones formales</p>
        </version>
