                <td class="cuerpo_tabla_centro">15,00</td>
              </tr>
              <tr>
                <td class="cuerpo_tabla_izq">CAJA 3 PASTILLAS.</td>
                <td class="cuerpo_tabla_centro">1410</td>
                <td class="cuerpo_tabla_centro">141</td>
                <td class="cuerpo_tabla_centro">1,96</td>
                <td class="cuerpo_tabla_centro">70,50</td>
                <td class="cuerpo_tabla_centro">15,00</td>
              </tr>
              <tr>
                <td class="cuerpo_tabla_izq">CAJA 2 PASTILLAS.</td>
                <td class="cuerpo_tabla_centro">550</td>
                <td class="cuerpo_tabla_centro">55</td>
                <td class="cuerpo_tabla_centro">1,96</td>
