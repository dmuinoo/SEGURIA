      <bloque id="s2-6" tipo="encabezado" fecha_caducidad="20181227" titulo="Sección 2">
        <version id_norma="BOE-A-2014-9469" fecha_publicacion="20140917" fecha_vigencia="20140918">
          <p class="seccion">Sección 2.ª Errores detectados una vez efectuado el pago de operaciones presupuestarias y no presupuestarias</p>
        </version>
