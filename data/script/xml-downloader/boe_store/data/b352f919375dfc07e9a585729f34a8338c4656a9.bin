      <bloque id="ai-2" tipo="encabezado" titulo="ANEXO II">
        <version id_norma="BOE-A-2014-4514" fecha_publicacion="20140428" fecha_vigencia="20140429">
          <p class="anexo_num">ANEXO II</p>
          <p class="anexo_tit">Especies prohibidas</p>
