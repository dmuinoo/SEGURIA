        <version id_norma="BOE-A-2018-17773" fecha_publicacion="20181227" fecha_vigencia="20191228">
          <p class="parrafo_2">Dado en Madrid, el 21 de diciembre de 2018.</p>
          <p class="firma_rey">FELIPE R.</p>
          <p class="firma_ministro">La Ministra de Trabajo, Migraciones y Seguridad Social,</p>
          <p class="firma_ministro">MAGDALENA VALERIO CORDERO</p>
        </version>
      </bloque>
