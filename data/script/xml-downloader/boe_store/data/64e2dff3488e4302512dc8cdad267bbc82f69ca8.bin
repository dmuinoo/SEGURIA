      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2012-9282" fecha_publicacion="20120626" fecha_vigencia="20120701">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Personal del sector público limitativo</p>
        </version>
