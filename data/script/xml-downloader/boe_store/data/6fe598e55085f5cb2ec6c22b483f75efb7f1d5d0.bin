      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2005-11757" fecha_publicacion="20050531" fecha_vigencia="20050630">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Objeto y ámbito de aplicación</p>
