      <bloque id="ci-7" tipo="encabezado" titulo="CAPÍTULO III">
        <version id_norma="BOE-A-2004-20890" fecha_publicacion="20041125" fecha_vigencia="20041125">
          <p class="capitulo_num">CAPÍTULO III</p>
          <p class="capitulo_tit">De la gestión indirecta</p>
        </version>
