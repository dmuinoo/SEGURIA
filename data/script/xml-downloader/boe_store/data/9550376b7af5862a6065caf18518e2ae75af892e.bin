      <bloque id="s1-3" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2004-20890" fecha_publicacion="20041125" fecha_vigencia="20041125">
          <p class="seccion">Sección 1.ª Prohibición de vertidos</p>
        </version>
