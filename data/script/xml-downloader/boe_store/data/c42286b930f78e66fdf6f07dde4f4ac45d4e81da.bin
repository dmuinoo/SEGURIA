        <version id_norma="BOE-A-2013-1927" fecha_publicacion="20121231" fecha_vigencia="20130101">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Disposiciones en materia de juego</p>
        </version>
