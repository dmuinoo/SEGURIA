      <bloque id="ci-3" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2005-14443" fecha_publicacion="20041230" fecha_vigencia="20050119">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Principios generales</p>
        </version>
