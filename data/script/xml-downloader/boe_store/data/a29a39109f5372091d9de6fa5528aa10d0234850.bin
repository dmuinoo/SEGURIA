      <bloque id="cii" tipo="encabezado" titulo="CAPÍTULO II">
        <version id_norma="BOE-A-1998-8202" fecha_publicacion="19971231" fecha_vigencia="19980101">
          <p class="capitulo_num">CAPÍTULO II</p>
          <p class="capitulo_tit">Tributación individual</p>
