        <version id_norma="BOE-A-2007-8713" fecha_publicacion="20070403" fecha_vigencia="20070703">
          <p class="titulo_num">TÍTULO I</p>
          <p class="titulo_tit">Objeto, principios y ámbito de aplicación de la ley</p>
        </version>
