      <bloque id="ai" tipo="encabezado" titulo="ANEXO I">
        <version id_norma="BOE-A-2022-17470" fecha_publicacion="20221026" fecha_vigencia="20221027">
          <p class="anexo_num">ANEXO I</p>
          <p class="anexo_tit">Modelo de solicitud de la nacionalidad española por opción</p>
          <p class="centro_redonda">(Apartado 1, párrafo primero, disposición adicional 8.ª de la Ley 20/2022)</p>
          <p class="imagen">
            <img class="frame" data-pdf="03938_revisado.pdf" height="2693" src="/datos/imagenes/disp/2023/38/3842_12785432_1.png" width="1811"/>
