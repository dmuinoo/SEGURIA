      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2009-9047" fecha_publicacion="20090512" fecha_vigencia="20090513">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Simplificación y racionalización en actuaciones administrativas</p>
        </version>
