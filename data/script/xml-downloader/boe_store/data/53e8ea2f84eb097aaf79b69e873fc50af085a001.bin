      <bloque id="ciii" tipo="encabezado" titulo="CAPÍTULO III">
        <version id_norma="BOE-A-2014-12328" fecha_publicacion="20141128" fecha_vigencia="20150101">
          <p class="capitulo_num">CAPÍTULO III</p>
          <p class="capitulo_tit">Reglas de valoración</p>
        </version>
