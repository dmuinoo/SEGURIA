              <tr class="row_column_31">
                <td>
                  <p class="cabeza_tabla">Código</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Estación</p>
                </td>
                <td>
                  <p class="cabeza_tabla">Categoría 2022</p>
                </td>
              </tr>
            </thead>
            <tbody>
              <tr class="row_column_31">
                <td>
                  <p class="cuerpo_tabla_centro">50500</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_izq">CÓRDOBA.</p>
