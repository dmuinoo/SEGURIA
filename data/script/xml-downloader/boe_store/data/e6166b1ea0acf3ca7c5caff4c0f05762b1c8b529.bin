      <bloque id="ani" tipo="encabezado" titulo="ANEXO I">
        <version id_norma="BOE-A-2002-21185" fecha_publicacion="20021101" fecha_vigencia="20021102">
          <p class="anexo_num">ANEXO I</p>
          <p class="anexo_tit">Empresas con activos en transporte sujetas al procedimiento de liquidación y sus costes fijos acreditados para el año 2002</p>
