      <bloque id="sprimera-2" tipo="encabezado" titulo="Sección Primera">
        <version id_norma="BOE-A-2004-889" fecha_publicacion="20031231" fecha_vigencia="20040101">
          <p class="seccion">Sección Primera. Registro de solicitantes de vivienda protegida</p>
        </version>
