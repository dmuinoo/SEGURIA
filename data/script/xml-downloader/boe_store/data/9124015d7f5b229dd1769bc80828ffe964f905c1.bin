      <bloque id="s3-2" tipo="encabezado" titulo="Sección 3">
        <version id_norma="BOE-A-1997-28052" fecha_publicacion="19971231" fecha_vigencia="19980101">
          <p class="seccion">Sección 3.ª Impuestos Especiales</p>
        </version>
