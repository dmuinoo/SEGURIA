      <bloque id="ci-7" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-1997-28052" fecha_publicacion="19971231" fecha_vigencia="19980101">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Corporaciones Locales</p>
