      <bloque id="fi" tipo="firma" titulo="[firma]">
        <version id_norma="BOE-A-1997-18096" fecha_publicacion="19970809" fecha_vigencia="19970810">
          <p class="parrafo_2">Madrid, 29 de julio de 1997.-El Presidente, Juan Fernández-Armesto Fernández España.</p>
        </version>
