      <bloque id="s6" tipo="encabezado" titulo="Sección 6">
        <version id_norma="BOE-A-2008-3179" fecha_publicacion="20071228" fecha_vigencia="20080129">
          <p class="seccion">Sección 6.ª Traslados de las oficinas de farmacia</p>
        </version>
