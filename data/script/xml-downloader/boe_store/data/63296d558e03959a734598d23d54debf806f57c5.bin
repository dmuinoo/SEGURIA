      <bloque id="s2-5" tipo="encabezado" titulo="Sección 2">
        <version id_norma="BOE-A-1995-24262" fecha_publicacion="19951109" fecha_vigencia="19951110">
          <p class="seccion">Sección 2.ª Competencias de la Administración General del Estado</p>
