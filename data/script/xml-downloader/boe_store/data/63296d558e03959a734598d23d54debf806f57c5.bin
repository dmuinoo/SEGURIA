      <bloque id="cvi" tipo="encabezado" titulo="CAPÍTULO VI">
        <version id_norma="BOE-A-1995-24262" fecha_publicacion="19951109" fecha_vigencia="19951110">
          <p class="capitulo_num">CAPÍTULO VI</p>
          <p class="capitulo_tit">Protección del asegurado</p>
