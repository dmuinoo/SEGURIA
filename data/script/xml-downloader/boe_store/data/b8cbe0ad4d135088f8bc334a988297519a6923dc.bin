      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2003-4607" fecha_publicacion="20030210" fecha_vigencia="20030310">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">De las Administraciones Públicas competentes</p>
