        <version id_norma="BOE-A-2000-6882" fecha_publicacion="20000412" fecha_vigencia="20000413">
          <p class="parrafo_2">Madrid, 7 de abril de 2000.</p>
          <p class="firma_ministro">ÁLVAREZ-CASCOS FERNÁNDEZ</p>
          <p class="parrafo_2">Excmos. Sres. Ministros de Defensa, del Interior y de Fomento.</p>
        </version>
      </bloque>
    </texto>
