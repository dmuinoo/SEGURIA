        <version id_norma="BOE-A-2017-15848" fecha_publicacion="20171230" fecha_vigencia="20171231">
          <p class="parrafo_2">Dado en Madrid, el 29 de diciembre de 2017.</p>
          <p class="firma_rey">FELIPE R.</p>
          <p class="firma_ministro">La Ministra de Empleo y Seguridad Social,</p>
          <p class="firma_ministro">FÁTIMA BÁÑEZ GARCÍA</p>
        </version>
      </bloque>
