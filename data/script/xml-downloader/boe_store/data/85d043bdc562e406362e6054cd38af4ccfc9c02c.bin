      <bloque id="s1-2" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2015-945" fecha_publicacion="20141029" fecha_vigencia="20141030">
          <p class="seccion">Sección 1.ª Reducciones de la base imponible</p>
