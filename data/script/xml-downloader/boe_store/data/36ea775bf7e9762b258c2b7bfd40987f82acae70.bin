      <bloque id="an" tipo="encabezado" titulo="ANEJO. Protocolos de detalle.">
        <version id_norma="BOE-A-2006-6003" fecha_publicacion="20060404" fecha_vigencia="20060405">
          <p class="anexo_num">ANEJO</p>
          <p class="anexo_tit">Protocolos de detalle</p>
        </version>
