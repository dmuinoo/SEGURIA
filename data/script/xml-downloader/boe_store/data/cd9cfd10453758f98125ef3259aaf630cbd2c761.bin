      <bloque id="s4" tipo="encabezado" titulo="Sección 4">
        <version id_norma="BOE-A-2014-8926" fecha_publicacion="20140826" fecha_vigencia="20140827">
          <p class="seccion">Sección 4.ª Información y control parlamentario</p>
