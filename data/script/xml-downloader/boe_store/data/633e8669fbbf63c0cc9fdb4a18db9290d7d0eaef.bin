              <tr class="row_column_7">
                <td>
                  <p class="cuerpo_tabla_centro">Verde</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_centro">Verde</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_centro">V</p>
                </td>
              </tr>
