      <bloque id="s5" tipo="encabezado" titulo="Sección 5">
        <version id_norma="BOE-A-2013-10270" fecha_publicacion="20131003" fecha_vigencia="20131004">
          <p class="seccion">Sección 5. Fenómenos meteorológicos extremos</p>
        </version>
