        <version id_norma="BOE-A-2016-12223" fecha_publicacion="20161223" fecha_vigencia="20170104">
          <p class="parrafo_2">Vitoria-Gasteiz, 14 de diciembre de 2016.</p>
          <p class="firma_ministro">La Directora de Tráfico,</p>
          <p class="firma_ministro">Gabiñe Sáez Molinuevo.</p>
        </version>
      </bloque>
