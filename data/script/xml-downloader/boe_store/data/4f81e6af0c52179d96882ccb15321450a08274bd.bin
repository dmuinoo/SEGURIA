        <version id_norma="BOE-A-2001-19609" fecha_publicacion="20010928" fecha_vigencia="20011018">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Disposiciones generales</p>
        </version>
