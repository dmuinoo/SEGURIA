      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2001-4392" fecha_publicacion="20010306" fecha_vigencia="20010307">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Del ejercicio de la profesión</p>
        </version>
