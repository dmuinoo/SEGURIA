      <bloque id="ci-2" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2009-18358" fecha_publicacion="20091118" fecha_vigencia="20091119">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Comunicaciones electrónicas</p>
