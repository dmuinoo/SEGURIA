      <bloque id="ai-3" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2025-24369" fecha_publicacion="20251202" fecha_vigencia="20260115">
          <p class="anexo">ANEXO III</p>
          <p class="imagen">
            <img class="frame" data-pdf="18268-3_revisado.pdf" src="/datos/imagenes/disp/2025/289/24369_16373665_1.png" width="2126" height="2365"/>
          </p>
          <p class="imagen">
            <img class="frame" data-pdf="18268-3_revisado.pdf" src="/datos/imagenes/disp/2025/289/24369_16373665_2.png" width="2126" height="2567"/>
          </p>
          <p class="imagen">
            <img class="frame" data-pdf="18268-3_revisado.pdf" src="/datos/imagenes/disp/2025/289/24369_16373665_3.png" width="2126" height="2307"/>
