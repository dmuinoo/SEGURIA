      <bloque id="ana" tipo="encabezado" titulo="ANEXO A">
        <version id_norma="BOE-A-2015-1456" fecha_publicacion="20150128" fecha_vigencia="20150205">
          <p class="anexo_num">ANEXO A</p>
          <p class="anexo_tit">Calendario de restricciones para la celebración de pruebas y actividades deportivas y recreativas o de cualquier tipo que supongan afectaciones al tráfico ordinario</p>
        </version>
