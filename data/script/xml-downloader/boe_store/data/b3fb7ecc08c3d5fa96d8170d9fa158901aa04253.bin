        <version id_norma="BOE-A-2020-12090" fecha_publicacion="20201009" fecha_vigencia="20201009">
          <p class="anexo">ANEXO VIII</p>
          <p class="imagen">
            <img class="frame-12" alt="1" src="/datos/imagenes/disp/2020/267/12090_7500.png" height="2698" width="2067"/>
