        <version id_norma="BOE-A-2016-8345" fecha_publicacion="20160810" fecha_vigencia="20160811">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Disposiciones generales</p>
        </version>
