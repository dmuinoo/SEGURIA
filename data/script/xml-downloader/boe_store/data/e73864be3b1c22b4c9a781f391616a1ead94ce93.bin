      <bloque id="aniii" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2002-10337" fecha_publicacion="20020530" fecha_vigencia="20020601">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Dietas en el extranjero según grupos y países</p>
          <table class="tabla">
            <tr>
              <td rowspan="2">
</td>
              <td colspan="3">
                <p class="cabeza_tabla">Cuantías en euros</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cabeza_tabla">Por alojamiento</p>
