                      <eli:format rdf:resource="http://www.iana.org/assignments/media-types/text/html"/>
                      <eli:embodies rdf:resource="https://www.boe.es/eli/es/rdl/2019/12/27/18/con/20191228/spa"/>
                    </eli:Format>
                  </eli:is_embodied_by>
