        <version id_norma="BOE-A-2010-738" fecha_publicacion="20091228" fecha_vigencia="20091229">
          <p class="titulo_num">TÍTULO I</p>
          <p class="titulo_tit">Clasificación de los establecimientos comerciales</p>
