              <tr>
                <th class="cabeza_tabla">
                  <p class="cabeza_tabla">Base liquidable</p>
                </th>
                <th class="cabeza_tabla">
                  <p class="cabeza_tabla">Euros por unidad contaminantes</p>
                </th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>
                  <p class="cuerpo_tabla_izq">Hasta 10 unidades contaminantes</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_der">5.000</p>
                </td>
              </tr>
