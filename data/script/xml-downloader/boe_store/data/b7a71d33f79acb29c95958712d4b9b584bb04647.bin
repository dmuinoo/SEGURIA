          </blockquote>
        </version>
      </bloque>
      <bloque id="dfsegunda" tipo="precepto" titulo="Disposición final segunda">
        <version id_norma="BOE-A-2002-24911" fecha_publicacion="20021221" fecha_vigencia="20021221">
