      <bloque id="civ-2" tipo="encabezado" titulo="CAPÍTULO IV">
        <version id_norma="BOE-A-2013-1927" fecha_publicacion="20121231" fecha_vigencia="20130101">
          <p class="capitulo_num">CAPÍTULO IV</p>
          <p class="capitulo_tit">Tributos sobre el juego</p>
        </version>
