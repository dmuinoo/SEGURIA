      <bloque id="tii-4" tipo="encabezado" titulo="TÍTULO II">
        <version id_norma="BOE-A-2014-90385" fecha_publicacion="20140802" fecha_vigencia="20140803">
          <p class="titulo_num">TÍTULO II</p>
          <p class="titulo_tit">Normas de gestión</p>
        </version>
