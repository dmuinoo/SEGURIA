      <bloque id="ci-2" tipo="encabezado" titulo="CAPÍTULO II">
        <version id_norma="BOE-A-1997-1256" fecha_publicacion="19961108" fecha_vigencia="19961109">
          <p class="capitulo_num">CAPÍTULO II</p>
          <p class="capitulo_tit">Régimen de actividades</p>
        </version>
