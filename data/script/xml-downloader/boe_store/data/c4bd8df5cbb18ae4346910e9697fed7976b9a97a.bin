      <bloque id="ai-4" tipo="encabezado" titulo="ANEXO IV">
        <version id_norma="BOE-A-2018-14900" fecha_publicacion="20181031" fecha_vigencia="20181101">
          <p class="anexo">ANEXO IV</p>
          <p class="imagen">
            <img class="frame-20" alt="4" src="/datos/imagenes/disp/2018/263/14900_8041.png" height="2695" width="2086"/>
