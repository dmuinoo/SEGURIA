      <bloque id="s3-2" tipo="encabezado" titulo="Sección 3">
        <version id_norma="BOE-A-1999-24785" fecha_publicacion="19991230" fecha_vigencia="20000101">
          <p class="seccion">Sección 3.ª Régimen Económico Fiscal de Canarias</p>
        </version>
