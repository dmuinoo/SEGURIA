      <bloque id="ci-3" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-1999-1826" fecha_publicacion="19990126" fecha_vigencia="19990726">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Matriculación</p>
