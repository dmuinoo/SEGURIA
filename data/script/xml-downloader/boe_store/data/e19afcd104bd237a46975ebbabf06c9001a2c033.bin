              <td>
                <p class="cuerpo_tabla_izq">Altura de la placa</p>
              </td>
              <td>
                <p class="cuerpo_tabla_der">75</p>
              </td>
              <td>
                <p class="cuerpo_tabla_der">120</p>
              </td>
            </tr>
