        <version id_norma="BOE-A-2020-12176" fecha_publicacion="20201012" fecha_vigencia="20201013">
          <p class="anexo_num">ANEXO 5</p>
          <p class="anexo_tit">Prestaciones sanitarias complementarias</p>
          <p class="parrafo_2">1. Prestaciones dentarias.</p>
          <table class="tabla_b_sica">
