      <bloque id="s4a-2" tipo="encabezado" titulo="Sección 4ª">
        <version id_norma="BOE-A-2014-13256" fecha_publicacion="20141220" fecha_vigencia="20150101">
          <p class="seccion">Sección 4.ª Ayuda asociada para las explotaciones de vacuno de leche</p>
        </version>
