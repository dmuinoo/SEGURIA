      <bloque id="ci-2" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2014-13256" fecha_publicacion="20141220" fecha_vigencia="20150101">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Régimen de pago básico</p>
