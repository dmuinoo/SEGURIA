      <bloque id="ci-8" tipo="encabezado" titulo="CAPÍTULO II">
        <version id_norma="BOE-A-2022-22128" fecha_publicacion="20221224" fecha_vigencia="20230101">
          <p class="capitulo_num">CAPÍTULO II</p>
          <p class="capitulo_tit">De los regímenes retributivos</p>
        </version>
