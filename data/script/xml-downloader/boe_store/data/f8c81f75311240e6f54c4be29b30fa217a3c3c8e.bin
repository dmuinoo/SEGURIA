      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2012-9283" fecha_publicacion="20120629" fecha_vigencia="20120629">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Impuesto sobre la Renta de las Personas Físicas</p>
