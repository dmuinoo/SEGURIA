        <version id_norma="BOE-A-1994-28967" fecha_publicacion="19941231" fecha_vigencia="19950120">
          <p class="titulo_num">TÍTULO I</p>
          <p class="titulo_tit">De la aprobación de los Presupuestos y de sus modificaciones</p>
