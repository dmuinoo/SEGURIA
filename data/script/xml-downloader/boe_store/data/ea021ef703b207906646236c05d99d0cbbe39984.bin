      <bloque id="s1" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2014-11996" fecha_publicacion="20141110" fecha_vigencia="20141111">
          <p class="seccion">Sección 1.ª Impuestos propios</p>
