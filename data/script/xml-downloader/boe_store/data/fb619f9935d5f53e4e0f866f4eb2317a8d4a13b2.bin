      <bloque id="cu-2" tipo="encabezado" titulo="CAPÍTULO ÚNICO">
        <version id_norma="BOE-A-2022-758" fecha_publicacion="20211230" fecha_vigencia="20220101">
          <p class="capitulo_num">CAPÍTULO ÚNICO</p>
          <p class="capitulo_tit">Tasa por servicios administrativos en materia de industria, energía y minas</p>
