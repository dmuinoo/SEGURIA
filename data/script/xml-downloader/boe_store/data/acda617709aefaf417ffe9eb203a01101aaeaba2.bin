                <td class="cell_style_override_4">
                  <p class="cuerpo_tabla_centro">8</p>
                </td>
              </tr>
              <tr class="row_column_6">
                <td class="cell_style_override_9">
                  <p class="cuerpo_tabla_der">30.000</p>
                </td>
                <td class="cell_style_override_10">
                  <p class="cuerpo_tabla_der">2.400</p>
                </td>
