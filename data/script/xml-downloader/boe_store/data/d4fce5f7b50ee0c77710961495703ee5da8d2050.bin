            <colgroup>
              <col width="50%"/>
              <col width="50%"/>
            </colgroup>
            <tbody>
              <tr>
                <td class="cuerpo_tabla_centro">Lugar y fecha</td>
                <td class="cuerpo_tabla_centro">Firma representante/s legal/es de la entidad</td>
              </tr>
            </tbody>
          </table>
        </version>
      </bloque>
