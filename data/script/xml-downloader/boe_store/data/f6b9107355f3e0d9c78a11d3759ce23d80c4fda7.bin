      <bloque id="fi" tipo="firma" titulo="[firma]">
        <version id_norma="BOE-A-2010-18068" fecha_publicacion="20101124" fecha_vigencia="20101125">
          <p class="parrafo_2">Dado en Madrid, el 15 de noviembre de 2010.</p>
          <p class="firma_rey">JUAN CARLOS R.</p>
          <p class="firma_ministro">El Ministro de la Presidencia,</p>
          <p class="firma_ministro">RAMÓN JÁUREGUI ATONDO</p>
        </version>
