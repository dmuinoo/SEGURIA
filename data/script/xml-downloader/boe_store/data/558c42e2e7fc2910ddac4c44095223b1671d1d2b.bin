            <tr>
              <td>
                <p class="cabeza_tabla">Tramo</p>
              </td>
              <td>
                <p class="cabeza_tabla">Tipo aplicable</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">28.500.001 a	285.700.000</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">0,2450</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">285.700.001 a 1.428.500.000</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">0,2275</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">1.428.500.001 a 2.857.100.000</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">0,1925</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_izq">2.857.100.001 a 5.714.200.000</p>
              </td>
