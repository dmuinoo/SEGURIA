      <bloque id="cv" tipo="encabezado" titulo="CAPÍTULO V">
        <version id_norma="BOE-A-1997-28053" fecha_publicacion="19971231" fecha_vigencia="19980101">
          <p class="capitulo_num">CAPÍTULO V</p>
          <p class="capitulo_tit">Otras acciones administrativas</p>
