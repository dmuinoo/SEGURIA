      <bloque id="s1-2" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2020-17266" fecha_publicacion="20201230" fecha_vigencia="20210101">
          <p class="seccion">Sección 1.ª Servicios financieros</p>
