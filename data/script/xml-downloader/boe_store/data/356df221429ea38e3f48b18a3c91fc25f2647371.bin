        <version id_norma="BOE-A-1995-10881" fecha_publicacion="19950508" fecha_vigencia="19950509">
          <p class="anexo_num">ANEXO</p>
          <p class="anexo_tit">SOLICITUD PARA LA AUTORIZACIÓN DE AGENCIAS DE COLOCACIÓN SIN FINES LUCRATIVOS</p>
