      <bloque id="cv-2" tipo="encabezado" titulo="CAPÍTULO V">
        <version id_norma="BOE-A-2013-10074" fecha_publicacion="20130928" fecha_vigencia="20130929">
          <p class="capitulo_num">CAPÍTULO V</p>
          <p class="capitulo_tit">Traslado intraempresarial</p>
