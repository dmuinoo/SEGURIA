      <bloque id="ciii-4" tipo="encabezado" titulo="CAPÍTULO III">
        <version id_norma="BOE-A-2013-10074" fecha_publicacion="20130928" fecha_vigencia="20130929">
          <p class="capitulo_num">CAPÍTULO III</p>
          <p class="capitulo_tit">Emprendedores y actividad empresarial</p>
