      <bloque id="aniii" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2006-3377" fecha_publicacion="20060225" fecha_vigencia="20060226">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Métodos considerados en el apartado 2 del artículo 3</p>
          <table class="tabla">
            <tr>
