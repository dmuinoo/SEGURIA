        <version id_norma="BOE-A-2008-18993" fecha_publicacion="20081125" fecha_vigencia="20081126">
          <p class="anexo_num">ANEXO II</p>
          <p class="anexo_tit">Espacios</p>
          <p class="centro_cursiva">Espacio formativo</p>
          <p class="parrafo_2">Aula polivalente.</p>
          <p class="parrafo">Sala de cata.</p>
          <p class="parrafo">Taller – almazara – bodega.</p>
