              <tr>
                <td>
                  <p class="cuerpo_tabla_centro">JOAN SAURA I LAPORTA,</p>
                  <p class="cuerpo_tabla_centro">Consejero de Relaciones Institucionales y Participación</p>
                </td>
                <td>
                  <p class="cuerpo_tabla_centro">PASQUAL MARAGALL I MIRA,</p>
                  <p class="cuerpo_tabla_centro">Presidente</p>
                </td>
              </tr>
            </tbody>
          </table>
          <p class="Tcuerpo_tabla"> </p>
        </version>
      </bloque>
    </texto>
