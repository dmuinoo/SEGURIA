        <version id_norma="BOE-A-1999-350" fecha_publicacion="19980703" fecha_vigencia="19980704">
          <p class="parrafo_2">Santiago de Compostela, 30 de junio de 1998.</p>
          <p class="firma_ministro"> </p>
          <p class="firma_ministro">MANUEL FRAGA IRIBARNE</p>
          <p class="firma_ministro">Presidente</p>
        </version>
      </bloque>
