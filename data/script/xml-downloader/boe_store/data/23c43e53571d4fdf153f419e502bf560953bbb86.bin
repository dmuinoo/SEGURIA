            <tr>
              <td>
                <p class="cabeza_tabla">Aeropuerto</p>
              </td>
              <td>
                <p class="cabeza_tabla">Día de la semana</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_centro">Gran Canaria</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">Martes</p>
