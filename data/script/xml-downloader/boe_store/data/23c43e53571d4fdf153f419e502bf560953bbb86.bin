            <tr>
              <td>
                <p class="cabeza_tabla">Grupo/subgrupo</p>
              </td>
              <td>
                <p class="cabeza_tabla">Grupo profesional</p>
                <p class="cabeza_tabla">personal laboral</p>
              </td>
            </tr>
            <tr>
              <td>
                <p class="cuerpo_tabla_centro">A1</p>
              </td>
              <td>
                <p class="cuerpo_tabla_centro">1</p>
              </td>
