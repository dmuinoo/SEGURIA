      <bloque id="ani" tipo="encabezado" titulo="ANEXO I">
        <version id_norma="BOE-A-2015-9208" fecha_publicacion="20150724" fecha_vigencia="20150813">
          <p class="anexo_num">ANEXO I</p>
          <p class="anexo_tit">Actividades sometidas a régimen de declaración responsable</p>
