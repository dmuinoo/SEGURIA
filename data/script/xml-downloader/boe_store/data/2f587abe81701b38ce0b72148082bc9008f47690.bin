                      <eli:format rdf:resource="http://www.iana.org/assignments/media-types/text/html"/>
                      <eli:embodies rdf:resource="https://www.boe.es/eli/es/rd/1999/01/22/81/con/19990203/spa"/>
                    </eli:Format>
                  </eli:is_embodied_by>
