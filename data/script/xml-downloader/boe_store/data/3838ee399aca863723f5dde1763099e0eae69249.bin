      <bloque id="ani" tipo="encabezado" titulo="ANEXO I">
        <version id_norma="BOE-A-2012-90047" fecha_publicacion="20121128" fecha_vigencia="20121128">
          <p class="anexo">ANEXO I</p>
          <p class="parrafo_2">Provincia de Almería:</p>
