      <bloque id="s3" tipo="encabezado" titulo="Sección 3">
        <version id_norma="BOE-A-2003-21941" fecha_publicacion="20031112" fecha_vigencia="20031113">
          <p class="seccion">Sección 3.ª Infracciones en materia de pesca continental</p>
        </version>
