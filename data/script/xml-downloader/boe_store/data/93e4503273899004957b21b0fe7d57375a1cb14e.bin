      <bloque id="aniv" tipo="encabezado" titulo="ANEXO IV">
        <version id_norma="BOE-A-2003-5956" fecha_publicacion="20030325" fecha_vigencia="20030401">
          <p class="anexo_num">ANEXO IV</p>
