      <bloque id="anii" tipo="encabezado" titulo="ANEXO II">
        <version id_norma="BOE-A-2004-2222" fecha_publicacion="20040206" fecha_vigencia="20040207">
          <p class="anexo_num">ANEXO II</p>
          <p class="anexo_tit">Modelo de solicitud de convalidación de la formación de Entrenador Deportivo anterior a la Orden de 5 de julio de 1999</p>
          <p class="imagen">
            <img src="/datos/imagenes/disp/2004/32/02222_002.png"/>
