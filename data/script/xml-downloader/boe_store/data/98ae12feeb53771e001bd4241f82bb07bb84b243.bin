      <bloque id="ai-3" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2022-3413" fecha_publicacion="20211229" fecha_vigencia="20220101">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Operaciones y actividades estadísticas consolidadas</p>
