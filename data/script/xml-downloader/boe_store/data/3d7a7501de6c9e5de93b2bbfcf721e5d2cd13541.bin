      <bloque id="soctava" tipo="encabezado" titulo="Sección octava">
        <version id_norma="BOE-A-2005-8986" fecha_publicacion="20050601" fecha_vigencia="20050602">
          <p class="seccion">Sección octava. Normas particulares sobre recaudación en período voluntario</p>
        </version>
