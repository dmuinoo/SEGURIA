      <bloque id="ci" tipo="encabezado" titulo="CAPÍTULO I">
        <version id_norma="BOE-A-2022-4919" fecha_publicacion="20220119" fecha_vigencia="20220208">
          <p class="capitulo_num">CAPÍTULO I</p>
          <p class="capitulo_tit">Medidas tributarias</p>
