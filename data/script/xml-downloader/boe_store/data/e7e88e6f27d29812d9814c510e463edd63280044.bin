      <bloque id="ss1-2" tipo="encabezado" titulo="Subsección 1">
        <version id_norma="BOE-A-2016-3191" fecha_publicacion="20160219" fecha_vigencia="20160319">
          <p class="subseccion">Subsección 1.ª Régimen</p>
        </version>
