      <bloque id="s1-6" tipo="encabezado" titulo="Sección 1">
        <version id_norma="BOE-A-2001-24965" fecha_publicacion="20011231" fecha_vigencia="20020101">
          <p class="seccion">Sección 1.ª De la gestion financiera</p>
        </version>
