      <bloque id="aniii" tipo="encabezado" titulo="ANEXO III">
        <version id_norma="BOE-A-2010-10710" fecha_publicacion="20100706" fecha_vigencia="20100707">
          <p class="anexo_num">ANEXO III</p>
          <p class="anexo_tit">Modelos de documentación estadístico-contable correspondiente a los contratos de seguro que instrumentan compromisos de pensiones</p>
          <p class="imagen">
            <img src="/datos/imagenes/disp/2010/163/10710_029.png"/>
