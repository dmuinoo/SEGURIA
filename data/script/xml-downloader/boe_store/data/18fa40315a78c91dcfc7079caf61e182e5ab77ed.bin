        <version id_norma="BOE-A-2014-13180" fecha_publicacion="20141219" fecha_vigencia="20141220">
          <p class="anexo">ANEXO I</p>
          <p class="imagen">
            <img class="imagen" src="/datos/imagenes/disp/2014/306/13180_001.png" alt="Imagen: img/disp/2014/306/13180_001.png"/>
          </p>
          <p class="imagen">
            <img class="imagen" src="/datos/imagenes/disp/2014/306/13180_002.png" alt="Imagen: img/disp/2014/306/13180_002.png"/>
